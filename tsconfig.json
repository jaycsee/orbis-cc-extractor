{
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "module": "CommonJS",
    "moduleResolution": "node16",
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  }
}
